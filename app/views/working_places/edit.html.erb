<h2>拠点フォーム編集</h2>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for(@working_place) do |f| %>

        <%= f.label :拠点番号 %>
        <%= f.number_field :working_place_number, class: 'form-control' %>

        <%= f.label :拠点名 %>
        <%= f.text_field :working_place_name, class: 'form-control' %>

        <%= f.label :勤怠種類 %>
        <%= f.text_field :working_place_type, class: 'form-control' %>

        <%= f.submit "更新", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>